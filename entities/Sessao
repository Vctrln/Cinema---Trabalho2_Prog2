package entities;

import java.time.LocalDateTime;
import java.util.Date;

public class Sessao{
	private Date dataHora;
	private Filme filme;
	private Sala sala;
	
	public Sessao(Date dataHora, Filme filme, Sala sala) {
		this.dataHora = dataHora;
		this.filme = filme;
		this.sala = sala;
	}

	public Date getDataHora() {
		return dataHora;
	}

	public void setDataHora(Date dataHora) {
		this.dataHora = dataHora;
	}

	public Filme getFilme() {
		return filme;
	}

	public void setFilme(Filme filme) {
		this.filme = filme;
	}

	public Sala getSala() {
		return sala;
	}

	public void setSala(Sala sala) {
		this.sala = sala;
	}
	
	
	
	}
