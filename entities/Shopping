package entities;

public class Shopping extends Localizacao {
	private String nome;

	public Shopping(String nome, String endereco, String bairro, String cidade) {
		super(cidade, bairro, endereco);
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	
}
